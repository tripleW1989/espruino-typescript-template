{
    "name": "espruino-typescript-await",
    "version": "1.0.0",
    "description": "Espruino async DHT22 sensor value reader in typescript",
    "repository": {
        "url": "https://github.com/VitriolCZ/espruino-typescript-await.git"
    },
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -cw",
        "deploy": "espruino --no-ble -b 115200 -p /dev/tty.usbserial-10 dist/bundle.js --board ESP8266_4MB --verbose",
        "deploy-watch": "espruino --no-ble -w -b 115200 -p /dev/tty.usbserial-10 dist/bundle.js --board ESP8266_4MB --verbose",
        "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --watch -r ts-node/register 'src/**/*.spec.ts' --watch-files 'src/**/*.ts'",
        "lint": "eslint ./src/**/*.ts --fix",
        "out": "espruino --board ESP8266_4MB --minify ./dist/bundle.js -o output.js -v"
    },
    "keywords": [
        "espruino",
        "typescript",
        "async",
        "await",
        "DHT22"
    ],
    "author": "Jakub Ranosz",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@types/bonjour": "^3.5.9",
        "@types/chai": "^4.2.14",
        "@types/chai-as-promised": "7.1.0",
        "@types/espruino": "^1.94.3",
        "@types/mocha": "^8.0.3",
        "@types/node": "^13.9.5",
        "@types/node-schedule": "^1.3.2",
        "@types/qrcode-terminal": "^0.12.0",
        "@types/sinon": "4.3.1",
        "@types/sinon-chai": "3.2.3",
        "@types/ws": "^8.2.2",
        "@typescript-eslint/eslint-plugin": "4.2.0",
        "@typescript-eslint/parser": "4.2.0",
        "axios": "^0.21.1",
        "babel-core": "6.26.3",
        "babel-plugin-transform-async-to-promises": "0.8.15",
        "bonjour": "^3.5.0",
        "chai": "^4.2.0",
        "chai-as-promised": "7.1.1",
        "commander": "^5.1.0",
        "espruino": "^0.1.34",
        "fast-async": "^6.3.8",
        "fs": "^0.0.1-security",
        "getmac": "^5.20.0",
        "mocha": "^8.2.0",
        "mqtt": "^4.3.4",
        "node-schedule": "^2.0.0",
        "pauseable": "^0.3.2",
        "qrcode-terminal": "^0.12.0",
        "rollup": "^2.66.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-filesize": "^9.1.2",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-sizes": "^1.0.4",
        "rollup-plugin-terser": "^5.3.1",
        "rollup-plugin-typescript": "1.0.1",
        "rxjs": "^7.5.2",
        "sinon": "9.0.3",
        "sinon-chai": "3.5.0",
        "timers": "^0.1.1",
        "ts-node": "^9.0.0",
        "tslib": "^2.3.1",
        "typescript": "4.0.5",
        "wireless-tools": "^0.19.0",
        "ws": "^7.4.6"
    },
    "dependencies": {
        "rollup-plugin-node-builtins": "^2.1.2"
    }
}
